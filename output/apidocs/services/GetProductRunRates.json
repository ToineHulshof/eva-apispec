{
  "name": "GetProductRunRates",
  "description": "Calculates the run-rates for the provided products for the provided warehouse OrganizationUnit, based on a date (\u0060Since\u0060).\n\nThe run-rates are returned per SoldFromOrganizationUnit.",
  "request": {
    "properties": [
      {
        "name": "ProductIDs",
        "description": null,
        "type": "array"
      },
      {
        "name": "Since",
        "description": null,
        "type": "date"
      },
      {
        "name": "WarehouseOrganizationUnitID",
        "description": null,
        "type": "int64"
      }
    ]
  },
  "response": {
    "properties": [
      {
        "name": "DailyRunRate",
        "description": null,
        "type": "float64"
      },
      {
        "name": "MonthlyRunRate",
        "description": null,
        "type": "float64"
      },
      {
        "name": "RunRates",
        "description": null,
        "type": "array"
      },
      {
        "name": "TotalQuantity",
        "description": null,
        "type": "int32"
      }
    ]
  }
}