    "properties": [
      {
        "name": "OrganizationUnitID",
        "description": null,
        "type": "int64"
      },
      {
        "name": "ProductIDs",
        "description": null,
        "type": "array"
      },
      {
        "name": "StockLabelIDs",
        "description": null,
        "type": "array"
      }
    ]
  },
  "response": {
